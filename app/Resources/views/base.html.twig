            }

        }

        function jsLoader(url) {
            if (typeof url !== 'undefined') {
                if (url.indexOf("{{ path('edit_profile') }}") !== -1) {
                    $.getScript("{{ asset('public/js/newEmployer.js') }}").done(function () {
                        startEmployer();
                    });

                    return true;
                } else if (url.indexOf("/daviplata/") !== -1) {
                    $.getScript("{{ asset('public/js/daviplata.js') }}").done(function () {
                        startDaviplata();
                    });

                    return true;
                } else if (url.indexOf("{{ path('register_employee') }}") !== -1) {
                    $.getScript("{{ asset('public/js/newEmployee.js') }}").done(function () {
                        startEmployee();
                    });

                    return true;
                } else if (url.indexOf("{{ path('manage_employees') }}") !== -1) {
                    $.getScript("{{ asset('public/js/manageEmployees.js') }}").done(function () {
                        manageEmployees();
                    });
                    return true;
                } else if (url.indexOf("{{ path('edit_employer') }}") !== -1) {
                    $.getScript("{{ asset('public/js/editEmployer.js') }}").done(function () {
                        startEmployerEdit();
                    });

                    return true;
                } else if (url.indexOf("{{ path('show_calculator_form') }}") !== -1) {
                    $.getScript("{{ asset('public/js/calculator.js') }}").done(function () {
                        calculatorCalculate();
                    });

                    return true;
                } else if (url.indexOf("{{ path('matrix_choose') }}") !== -1) {
                    $.getScript("{{ asset('public/js/afiliation.js') }}").done(function () {
                        startAfiliation();
                    });

                    return true;
                } else if (url.indexOf("{{ path('payments_method') }}") !== -1) {
                    $.getScript("{{ asset('public/js/paymentMethod.js') }}").done(function () {
                        startPayment();
                    });

                    return true;
                } else if (url.indexOf("{{ path('novelty_select') }}") !== -1) {
                    $.getScript("{{ asset('public/js/novelty.js') }}").done(function () {
                        startNovelty();
                    });

                    return true;
                } else if (url.indexOf("{{ path('show_user') }}") !== -1) {
                    $.getScript("{{ asset('public/js/myAccount.js') }}").done(function () {
                        startMyAccount();
                    });

                    return true;
                } else if (url.lastIndexOf("{{ path('subscription_choices') }}") !== -1) {
                    $.getScript("{{ asset('public/js/subscriptionChoise.js') }}").done(function () {
                        startSubscriptionChoise();
                    });

                    return true;
                } else if (url.lastIndexOf("{{ path('subscription_confirm') }}") !== -1) {
                    $.getScript("{{ asset('public/js/subscriptionActivate.js') }}").done(function () {
                        startSubscriptionActivate();
                    });

                    return true;
                } else if (url.lastIndexOf("/liquidations/final") !== -1) {
                    $.getScript("{{ asset('public/js/liquidation.js') }}").done(function () {
                        validateLiqForm();
                    });

                    return true;
                } else if (url.lastIndexOf("/contact") !== -1) {
                    $.getScript("{{ asset('public/js/contact.js') }}").done(function () {
                        startContact();
                    });

                    return true;
                }

                return false;
            }
            return false;
        }
        function getParameterByName(name) {
            name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
            var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
                    results = regex.exec(location.search);
            return results === null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
        }
