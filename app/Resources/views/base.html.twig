                function sendAjax(url, form) {

                    $.ajax({
                        url: url,
                        type: $(form).attr('method'),
                        data: $(form).serializeArray(),
                        statusCode: {
                            500: function () {
                                alert("Lo sentimos pero estamos teniendo problemas con esta funcionalidad, por favor intenta de nuevo más tarde.");
