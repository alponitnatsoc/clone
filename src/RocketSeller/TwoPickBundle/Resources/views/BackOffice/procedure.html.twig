									{% if employeeInfoFinished is defined and employeeInfoFinished == 1  and docsFinished is defined and docsFinished == 2 and employeeEntitiesFinished == employeeEntitiesCount and infoFinished is defined and infoFinished == 2 and employeeDocsFinished is defined and employeeDocsFinished == 2 and entitiesFinished == employer.entities|length %}
