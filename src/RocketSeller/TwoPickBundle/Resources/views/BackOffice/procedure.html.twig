			border: 0px;
			margin: 0px 6px 0px 2px;
		}
		select[id="formInfoEmployee{{ ehe.idEmployerHasEmployee }}_expeditionDate_month"]{
			padding: 0;
			height: 20px;
			font-size: 12px;
			border: 0px;
			margin: 0px 6px 0px 2px;
		}
		select[id="formInfoEmployee{{ ehe.idEmployerHasEmployee }}_expeditionDate_year"]{
			padding: 0;
			height: 20px;
			font-size: 12px;
			border: 0px;
			margin: 0px 6px 0px 2px;
		}
		select[id="formInfoEmployee{{ ehe.idEmployerHasEmployee }}_birthDate_day"]{
			padding: 0;
			height: 20px;
			font-size: 12px;
			border: 0px;
			margin: 0px 6px 0px 2px;
		}
		select[id="formInfoEmployee{{ ehe.idEmployerHasEmployee }}_birthDate_month"]{
			padding: 0;
			height: 20px;
			font-size: 12px;
			border: 0px;
			margin: 0px 6px 0px 2px;
		}
		select[id="formInfoEmployee{{ ehe.idEmployerHasEmployee }}_birthDate_year"]{
			padding: 0;
			height: 20px;
			font-size: 12px;
			border: 0px;
			margin: 0px 6px 0px 2px;
		}
        #aditional-actions_{{ var1 }},#validate-info-employee_{{ var1 }},#validate-doc-employee_{{ var1 }},#validate-cat-employee_{{ var1 }},#eployee_{{ var1 }}{
			touch-action: manipulation;
			cursor: pointer;
			-webkit-user-select: none;
		}
		{% set var1 = var1 + 1 %}
		{% endfor %}

	</style>
	<!--Div where documents modal is rendered-->
	<div id="myModal" class="modal fade" role="dialog">
		<div id="modal-body">
		</div>
	</div>
	<!--End of modal rendering-->
	{% set procedure = procedure %}
	<div id="main" class="col-md -12">
		<!--Begin of Real Procedure-->
		<div class="back-panel-title">
			<p>Tramite No: {{ procedure.idProcedure }}</p>
		</div>
		<!--Begin of Employer Actions-->
		<div class="back-panel">
			<div id="hide_employer">
				<!--Employer Name-->
				<div class="backoffice-headding" >
					<h7>Empleador: {{ procedure.userUser.personPerson.fullName }}</h7>
				</div>
				<!--End of employer Name-->
				<table class="col-md-12 back-table">
					<!--Employer Table header-->
					<thead>
					<tr>
						<th class="th-title">Tramite</th>
						<th class="th-title">Estado</th>
						<th class="th-title">Fecha de Entrada</th>
						<th class="th-title">Dado de alta</th>
					</tr>
					</thead>
					<!--End of employer Table header-->
					<tbody>
					<!--Employer Actions begin-->
					<tr>
						<!--td styles based on action status-->
						{% set infoFinished = 0 %}
						{% set action_employer = procedure.getActionsByActionType(actionTypes['VER']).first %}
						{% if action_employer.priority == 0 %}
							{% set tdDateStyle = "background-color: rgba(74, 169, 36, 0.34);color: #145456;" %}
						{% elseif action_employer.priority == 1 %}
							{% set tdDateStyle = "background-color: rgba(169, 148, 36, 0.52);color: #563808;" %}
						{% elseif action_employer.priority >= 2%}
							{% set tdDateStyle = "background-color: rgba(224, 110, 110, 0.45);color: #c70707;" %}
						{% endif %}
						{% if action_employer.actionStatus.code == 'FIN' %}
							{% set tdStyle = "background-color: rgba(74, 169, 36, 0.34);color: #145456;" %}
							{% set infoFinished = 1 %}
						{% elseif  action_employer.actionStatus.code == 'NEW' %}
							{% set tdStyle = "" %}
						{% elseif  action_employer.actionStatus.code == 'CORT' %}
							{% set tdStyle = "background-color: rgba(169, 148, 36, 0.52);color: #563808;" %}
						{% elseif  action_employer.actionStatus.code == 'ERRO' %}
							{% set tdStyle = "background-color: rgba(224, 110, 110, 0.45);color: #c70707;" %}
						{% else %}
							{% set tdStyle = "" %}
						{% endif %}
						<!--End of td styles-->
						<!--validate info action-->
						<td class="td-title" style="{{ tdStyle }}">
							<div id="validate-info-employer">
								{{ action_employer.actionTypeName }}
							</div>
						</td>
						<td class="td-title" style="{{ tdStyle }}">
							{{ action_employer.status }}
						</td>
						<td class="td-title" style="{{ tdStyle }}">
							{{ action_employer.createdAt|date("m/d/Y") }}
						</td>
						<td class="td-title" style="{{ tdDateStyle }}">
							{% if action_employer.finishedAt is not null %}
								{{ action_employer.finishedAt|date("m/d/Y") }}
							{% else %}
								-
							{% endif %}
						</td>
					</tr>
					<tr>
						{% set action_employer = procedure.getActionsByActionType(actionTypes['VDDE']).first %}
						{% if action_employer.priority == 0 %}
							{% set tdDateStyle = "background-color: rgba(74, 169, 36, 0.34);color: #145456;" %}
						{% elseif action_employer.priority == 1 %}
							{% set tdDateStyle = "background-color: rgba(169, 148, 36, 0.52);color: #563808;" %}
						{% elseif action_employer.priority == 2%}
							{% set tdDateStyle = "background-color: rgba(224, 110, 110, 0.45);color: #c70707;" %}
						{% endif %}
						{% if action_employer.actionStatus.code == 'FIN' %}
							{% set tdStyle = "background-color: rgba(74, 169, 36, 0.34);color: #145456;" %}
							{% if infoFinished is defined and infoFinished == 1 %}
								{% set infoFinished = 2 %}
							{% endif %}
						{% elseif  action_employer.actionStatus.code == 'NEW' %}
							{% set tdStyle = "" %}
						{% elseif  action_employer.actionStatus.code == 'CORT' %}
							{% set tdStyle = "background-color: rgba(169, 148, 36, 0.52);color: #563808;" %}
						{% elseif  action_employer.actionStatus.code == 'ERRO' %}
							{% set tdStyle = "background-color: rgba(224, 110, 110, 0.45);color: #c70707;" %}
						{% else %}
							{% set tdStyle = "" %}
						{% endif %}
						<!--validate Document Action-->
						<td class="td-title" style="{{ tdStyle }}">
							<div id="validate-doc-employer">
								{{ action_employer.actionTypeName }}
							</div>
						</td>
						<td class="td-title" style="{{ tdStyle }}">
							{{ action_employer.status }}
						</td>
						<td class="td-title" style="{{ tdStyle }}">
							{{ action_employer.createdAt|date("m/d/Y") }}
						</td>
						<td class="td-title" style="{{ tdDateStyle }}">
							{% if action_employer.finishedAt is not null %}
								{{ action_employer.finishedAt|date("m/d/Y") }}
							{% else %}
								-
							{% endif %}
						</td>
					</tr>
					<!--validate info content and document content-->
					{% if infoFinished is defined and infoFinished == 2 %}
						{% set dysplayInfo = "display:none;" %}
					{% else %}
						{% set dysplayInfo = "" %}
					{% endif %}
					<tr id="employer-info-content" style="{{ dysplayInfo }}">
						<td colspan="4">
							<div class="back-section col-md-12">
								<!--Form employer info begin-->
								<div class="col-md-6" style="padding: 0px;margin: 0px 5px 0px 25px;">
									<div class="col-md-15 text-center content-title">
										Datos del Empleador
									</div>
									<div style="border: 3px solid #525659;margin-right: 6px; height: 480px; padding: 20px 0">
										{{ form_start(formDocument,{"attr":{'id':'form_employer_info'}}) }}
										<div class="col-md-15 row-content " style="margin-top: 30px">
											<div class="col-md-5 no-padding">
												{{ form_label(formDocument.document) }}
											</div>
											<div class="col-md-6 no-padding">
												{{ form_widget(formDocument.document) }}
											</div>
										</div>
										<div class="col-md-15 row-content">
											<div class="col-md-5 no-padding">
												{{ form_label(formDocument.documentType) }}
											</div>
											<div class="col-md-6 no-padding">
												{{ form_widget(formDocument.documentType) }}
											</div>
										</div>
										<div class="col-md-15 row-content">
											<div class="col-md-5 no-padding">
												{{ form_label(formDocument.name) }}
											</div>
											<div class="col-md-6 no-padding">
												{{ form_widget(formDocument.name) }}
											</div>
										</div>
										<div class="col-md-15 row-content">
											<div class="col-md-5 no-padding">
												{{ form_label(formDocument.lastName1) }}
											</div>
											<div class="col-md-6 no-padding">
												{{ form_widget(formDocument.lastName1) }}
											</div>
										</div>
										<div class="col-md-15 row-content">
											<div class="col-md-5 no-padding">
												{{ form_label(formDocument.lastName2) }}
											</div>
											<div class="col-md-6 no-padding">
												{{ form_widget(formDocument.lastName2) }}
											</div>
										</div>
										<div class="col-md-15 row-content">
											<div class="col-md-5 no-padding">
												{{ form_label(formDocument.expeditionDate) }}
											</div>
											<div class="col-md-6 no-padding">
												{{ form_widget(formDocument.expeditionDate) }}
											</div>
										</div>
										<div class="col-md-15 row-content">
											<div class="col-md-5 no-padding">
												{{ form_label(formDocument.birthDate) }}
											</div>
											<div class="col-md-6 no-padding">
												{{ form_widget(formDocument.birthDate) }}
											</div>
										</div>
										<div class="col-md-15 row-content">
											<div class="col-md-5 no-padding">
												{{ form_label(formDocument.email) }}
											</div>
											<div class="col-md-6 no-padding">
												{{ form_widget(formDocument.email) }}
											</div>
										</div>
										<div class="col-md-15 row-content">
											<div class="col-md-5 no-padding">
												{{ form_label(formDocument.phone) }}
											</div>
											<div class="col-md-6 no-padding">
												{{ form_widget(formDocument.phone) }}
											</div>
										</div>
										{% if infoFinished == 0 %}
											<div class="col-md-12 row-content text-center">
												<div class="col-md-10">
													{{ form_widget(formDocument.edit) }}
												</div>
											</div>
										{% endif %}
										{{ form_end(formDocument) }}
									</div>
								</div>
								<!--Form employer info end-->
								<!--Employer document dysplay-->
								<div class="col-md-5 text-center" style="padding: 0px;margin: 0px 20px 0px 10px">
									<div class="col-md-5 text-center content-title" style="width: 100%">
										{% if employer.personPerson.documentType == 'CC' %}
											Cédula del empleador
										{% elseif employer.personPerson.documentType == 'CE'%}
											Cédula de Extranjería del empleador
										{% elseif employer.personPerson.documentType == 'PASAPORTE'%}
											Pasaporte del empleador
										{% elseif employer.personPerson.documentType == 'TI' %}
											Tarjeta de identidad del empleador
										{% endif %}
									</div>
									{% if employer.personPerson.documentDocument %}
										{% if utils.getDocumentPath(employer.personPerson.documentDocument) == '' %}
											<div class="doc-message">
												{% if employer.personPerson.documentType == 'CC' %}
													<h7>LLamar al empleador y decirle que suba la cédula</h7>
												{% elseif employer.personPerson.documentType == 'CE'%}
													<h7>LLamar al empleador y decirle que suba la cédula de extranjería</h7>
												{% elseif employer.personPerson.documentType == 'PASAPORTE'%}
													<h7>LLamar al empleador y decirle que suba el pasaporte</h7>
												{% elseif employer.personPerson.documentType == 'TI' %}
													<h7>LLamar al empleador y decirle que suba la tarjeta de identidad</h7>
												{% endif %}
											</div>
										{% else %}
											<div style="border: 3px solid #525659;margin-top: 32px;width: 100%; height: 480px;font-size: 15px">
												<embed width="100%" height="474" src={{ utils.getDocumentPath( employer.personPerson.documentDocument) }}  alt=”pdf” pluginspage=”//www.adobe.com/products/acrobat/readstep2.html”>
												</embed>
											</div>
										{% endif %}
									{% else %}
										<div class="doc-message">
											{% if employer.personPerson.documentType == 'CC' %}
												<h7>LLamar al empleador y decirle que suba la cédula</h7>
											{% elseif employer.personPerson.documentType == 'CE'%}
												<h7>LLamar al empleador y decirle que suba la cédula de extranjería</h7>
											{% elseif employer.personPerson.documentType == 'PASAPORTE'%}
												<h7>LLamar al empleador y decirle que suba el pasaporte</h7>
											{% elseif employer.personPerson.documentType == 'TI' %}
												<h7>LLamar al empleador y decirle que suba la tarjeta de identidad</h7>
											{% endif %}
										</div>
									{% endif %}
								</div>
								<!--Employer document dysplay end-->
								<!--Employer info action buttons begin-->
								<div class="col-md-6 text-center" style="padding: 0px;margin: 20px 0px 0px 0px;">
									{% set action_employer = procedure.getActionsByActionType(actionTypes['VER']).first %}
									{% if action_employer.actionStatus.code == 'FIN' %}
										<a class="btn-backoffice notAjax" href="{{ path('activate_action',{'actionId':action_employer.idAction}) }}">
											Activar Tramite
										</a>
									{% else %}
										<a id="validate_info_employer_btn" class="btn-backoffice notAjax" href="{{ path('finish_action',{'actionId':action_employer.idAction}) }}">
											Validar Información
										</a>
										<h1 id="validate_info_employer_save" style="display: none; color: black;font-weight: 600;font-size: 16px">Guarde los cambios para poder validar la información</h1>
									{% endif %}
								</div>
								<!--validate employer action end-->
								<!--Document action buttons begin-->
								<div class="col-md-6 text-center" style="padding: 0px;margin: 20px 0px 0px 0px;">

									{% set action_employer = procedure.getActionsByActionType(actionTypes['VDDE']).first %}
									{% if action_employer.actionStatus.code == 'ERRO' %}
										{% if employerNotifications['notCC'].status == 0 %}
											<a class="btn-backoffice notAjax" href="{{ path('activate_notification',{ 'notificationId':employerNotifications['notCC'].id, 'actionId':action_employer.idAction }) }}">
												Activar Notificación
											</a>
										{% endif %}
										<a class="btn-backoffice-download notAjax" target="_blank" href="{{ path('export_document_by_id',{'idDoc':employer.personPerson.documentDocument.idDocument})}}">
											Descargar
										</a>
										<a class="btn-backoffice-upload notAjax download-link" href="{{ employerNotifications['notCC'].relatedLink }}/{{ employerNotifications['notCC'].id }}/{{ procedure.idProcedure }}">
											Subir Documento
										</a>
									{% elseif action_employer.actionStatus.code == 'CORT' %}
										<a class="btn-backoffice notAjax" href="{{ path('finish_action',{'actionId':action_employer.idAction}) }}">
											Validar Documento
										</a>
										<a class="btn-backoffice-download notAjax" target="_blank" href="{{ path('export_document_by_id',{'idDoc':employer.personPerson.documentDocument.idDocument})}}">
											Descargar
										</a>
										<a class="btn-backoffice-error notAjax" href="{{ path('error_action',{'actionId':action_employer.idAction}) }}">
											Error
										</a>
									{% elseif action_employer.actionStatus.code == 'FIN' %}
										<a class="btn-backoffice-download notAjax" target="_blank" href="{{ path('export_document_by_id',{'idDoc':employer.personPerson.documentDocument.idDocument})}}">
											Descargar
										</a>
										<a class="btn-backoffice-error notAjax" href="{{ path('error_action',{'actionId':action_employer.idAction}) }}">
											Error
										</a>
									{% elseif action_employer.actionStatus.code == 'DCPE' %}
										<a class="btn-backoffice-upload notAjax download-link" href="{{ employerNotifications['notCC'].relatedLink }}/{{ employerNotifications['notCC'].id }}/{{ procedure.idProcedure }}">
											Subir Documento
										</a>
									{% else %}
										<a class="btn-backoffice-upload notAjax download-link" href="{{ employerNotifications['notCC'].relatedLink }}/{{ employerNotifications['notCC'].id }}/{{ procedure.idProcedure }}">
											Subir Documento
										</a>
										<a class="btn-backoffice notAjax" href="{{ path('finish_action',{'actionId':action_employer.idAction}) }}">
											Validar Documento
										</a>
										<a class="btn-backoffice-download notAjax" target="_blank" href="{{ path('export_document_by_id',{'idDoc':employer.personPerson.documentDocument.idDocument})}}">
											Descargar
										</a>
										<a class="btn-backoffice-error notAjax" href="{{ path('error_action',{'actionId':action_employer.idAction}) }}">
											Error
										</a>
									{% endif %}
								</div>
								<!--Document action buttons end-->
							</div>
						</td>
					</tr>
					<tr>
						{% set docsFinished = 0 %}
						{% set action_employer = procedure.getActionsByActionType(actionTypes['VRTE']).first %}
						{% if action_employer.priority == 0 %}
							{% set tdDateStyle = "background-color: rgba(74, 169, 36, 0.34);color: #145456;" %}
						{% elseif action_employer.priority == 1 %}
							{% set tdDateStyle = "background-color: rgba(169, 148, 36, 0.52);color: #563808;" %}
						{% elseif action_employer.priority == 2%}
							{% set tdDateStyle = "background-color: rgba(224, 110, 110, 0.45);color: #c70707;" %}
						{% endif %}
						{% if action_employer.actionStatus.code == 'FIN' %}
							{% set tdStyle = "background-color: rgba(74, 169, 36, 0.34);color: #145456;" %}
							{% set docsFinished = 1 %}
						{% elseif  action_employer.actionStatus.code == 'NEW' %}
							{% set tdStyle = "" %}
						{% elseif  action_employer.actionStatus.code == 'CORT' %}
							{% set tdStyle = "background-color: rgba(169, 148, 36, 0.52);color: #563808;" %}
						{% elseif  action_employer.actionStatus.code == 'ERRO' %}
							{% set tdStyle = "background-color: rgba(224, 110, 110, 0.45);color: #c70707;" %}
