{% extends '::base.html.twig' %}

{% block body -%}
    <h1>User</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Facebook_id</th>
                <td>{{ entity.facebookid }}</td>
            </tr>
            <tr>
                <th>Google_id</th>
                <td>{{ entity.googleid }}</td>
            </tr>
            <tr>
                <th>Twitter_id</th>
                <td>{{ entity.twitterid }}</td>
            </tr>
            <tr>
                <th>Facebook_access_token</th>
                <td>{{ entity.facebookaccesstoken }}</td>
            </tr>
            <tr>
                <th>Google_access_token</th>
                <td>{{ entity.googleaccesstoken }}</td>
            </tr>
            <tr>
                <th>Twitter_access_token</th>
                <td>{{ entity.twitteraccesstoken }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('user') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('user_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
