
{% extends "::base.html.twig" %}
{% block fos_user_content%}
	{% if is_granted("IS_AUTHENTICATED_FULLY") %}
		<a href="{{ path('fos_user_security_logout') }}">{{ 'layout.logout'|trans({}, 'FOSUserBundle') }}</a>
		Welcome {{ app.user.username }}
	{% else %}
		Welcome Visitor!!
	{% endif %}
{% endblock %}